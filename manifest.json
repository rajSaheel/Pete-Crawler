  "action":{
    "default_popup":"index.html",
    "default_icon":"icon.png"
  },
  "author": "Saheel Raj",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "*://*/*","http://*/*", "https://*/*"
  ]
}